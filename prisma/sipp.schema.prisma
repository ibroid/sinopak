generator client {
    provider = "prisma-client-js"
    output   = "./generated/sipp_client"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL_SIPP")
}

model actiopaulianaweb {
    IDPerkara            BigInt    @id @db.UnsignedBigInt
    tglPendaftaranAction DateTime? @db.Date
    keterangan           String?   @db.VarChar(255)
}

model agama {
    id                   Int       @id @db.UnsignedInt
    nama                 String    @unique(map: "nama") @default("") @db.VarChar(50)
    keterangan           String?   @db.VarChar(255)
    aktif                String    @default("Y") @db.Char(1)
    diedit_oleh          String?   @db.VarChar(30)
    diedit_tanggal       DateTime? @db.DateTime(0)
    diinput_oleh         String?   @db.VarChar(30)
    diinput_tanggal      DateTime? @db.DateTime(0)
    diperbaharui_oleh    String?   @db.VarChar(30)
    diperbaharui_tanggal DateTime? @db.DateTime(0)
    pihak                pihak[]

    @@index([aktif], map: "aktif")
    @@index([diinput_tanggal], map: "diinput_tanggal")
    @@index([diperbaharui_tanggal], map: "diperbaharui_tanggal")
}

model agenda_sidang {
    id                   Int       @id @db.UnsignedInt
    alur_perkara_id      Int?      @db.UnsignedInt
    nama                 String    @unique(map: "nama") @default("") @db.VarChar(50)
    keterangan           String?   @db.VarChar(255)
    aktif                String    @default("Y") @db.Char(1)
    urutan               Int?      @db.UnsignedInt
    diedit_oleh          String?   @db.VarChar(30)
    diedit_tanggal       DateTime? @db.DateTime(0)
    diinput_oleh         String?   @db.VarChar(30)
    diinput_tanggal      DateTime? @db.DateTime(0)
    diperbaharui_oleh    String?   @db.VarChar(30)
    diperbaharui_tanggal DateTime? @db.DateTime(0)

    @@index([aktif], map: "aktif")
    @@index([diinput_tanggal], map: "diinput_tanggal")
    @@index([diperbaharui_tanggal], map: "diperbaharui_tanggal")
}

model alur_perkara {
    id                   Int             @id @db.UnsignedInt
    kode                 String?         @db.VarChar(20)
    nama                 String?         @db.VarChar(100)
    keterangan           String?         @db.VarChar(255)
    aktif                String          @default("Y") @db.Char(1)
    urutan               Int?            @db.UnsignedInt
    jenis_pengadilan     String?         @db.Char(1)
    diedit_oleh          String?         @db.VarChar(30)
    diedit_tanggal       DateTime?       @db.DateTime(0)
    diinput_oleh         String?         @db.VarChar(30)
    diinput_tanggal      DateTime?       @db.DateTime(0)
    diperbaharui_oleh    String?         @db.VarChar(30)
    diperbaharui_tanggal DateTime?       @db.DateTime(0)
    biaya_perkara        biaya_perkara[]
    jenis_dokumen        jenis_dokumen[]
    perkara              perkara[]

    @@index([diinput_tanggal], map: "diinput_tanggal")
    @@index([diperbaharui_tanggal], map: "diperbaharui_tanggal")
}

